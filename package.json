{
  "name": "forte-card-previewer",
  "version": "1.0.0",
  "description": "Interactive web-based card viewer with filtering, animations and likes system",
  "main": "index.html",
  "scripts": {
    "build:images": "node generate_image_structure.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./dist/output.css --minify",
    "watch:css": "tailwindcss -i ./src/css/input.css -o ./dist/output.css --watch",
    "lint": "eslint ."
  },
  "keywords": [
    "gallery",
    "cards",
    "forte",
    "viewer",
    "interactive"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.1"
  },
  "dependencies": {
    "three": "^0.152.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16"
  }
